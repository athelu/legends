{
  "id": "legends",
  "title": "Legends TTRPG System",
  "description": "A classless tabletop roleplaying game built entirely around the number eight, using d8 dice and a roll-under system with success counting.",
  "version": "0.6.0",
  "authors": [
    {
      "name": "Sean (athelu)",
      "url": "https://github.com/athelu/legends"
    }
  ],
  "compatibility": {
    "minimum": "13",
    "verified": "13"
  },
  "esmodules": [
    "module/legends.mjs"
  ],
  "styles": [
    "styles/legends.css"
  ],
  "packs": [
    {
      "name": "backgrounds",
      "label": "Legends - Backgrounds",
      "path": "packs/legends/backgrounds",
      "type": "Item",
      "system": "legends"
    },
    {
      "name": "ancestries",
      "label": "Legends - Ancestries",
      "path": "packs/legends/ancestries",
      "type": "Item",
      "system": "legends"
    },
    {
      "name": "traits",
      "label": "Legends - Traits",
      "path": "packs/legends/traits",
      "type": "Item",
      "system": "legends"
    },
    {
      "name": "flaws",
      "label": "Legends - Flaws",
      "path": "packs/legends/flaws",
      "type": "Item",
      "system": "legends"
    },
    {
      "name": "feats",
      "label": "Legends - Feats",
      "path": "packs/legends/feats",
      "type": "Item",
      "system": "legends"
    },
    {
      "name": "weapons",
      "label": "Legends - Weapons",
      "path": "packs/legends/weapons",
      "type": "Item",
      "system": "legends"
    },
    {
      "name": "armor",
      "label": "Legends - Armor",
      "path": "packs/legends/armor",
      "type": "Item",
      "system": "legends"
    },
    {
      "name": "equipment",
      "label": "Legends - Equipment",
      "path": "packs/legends/equipment",
      "type": "Item",
      "system": "legends"
    },
    {
      "name": "weaves",
      "label": "Legends - Weaves",
      "path": "packs/legends/weaves",
      "type": "Item",
      "system": "legends"
    },
    {
      "name": "action",
      "label": "Legends - Actions",
      "path": "packs/action",
      "type": "Item",
      "system": "legends"
    },
    {
      "name": "abilities",
      "label": "Legends - Abilities",
      "path": "packs/abilities",
      "type": "Item",
      "system": "legends"
    }
