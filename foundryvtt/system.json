      "character": {},
      "npc": {}
    },
    "Item": {
      "weapon": {},
      "armor": {},
      "equipment": {},
      "weave": {},
      "feat": {},
      "trait": {},
      "flaw": {},
      "background": {},
      "ancestry": {}
    }
  },
  "socket": false,
  "initiative": "1d8 + @attributes.agility.value + @luck.current + @skills.perception.value",
  "primaryTokenAttribute": "hp",
  "secondaryTokenAttribute": "energy",
